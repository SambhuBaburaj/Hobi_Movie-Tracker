    <div id="offcanvas-cart" class="offcanvas offcanvas-cart">
        <div class="inner">
            <div class="head">
                <span class="title">Cart</span>
                <button class="offcanvas-close">×</button>
            </div>
            <div class="body customScroll">
                <ul class="minicart-product-list">
                    <% let TotalPrice=0 %>
                    <% let totalQ=0,Price=0,totaldiscount=0 %>
                <% if(!cartdata[0]==0){ %> 
                   
                    <% locals.cartdata.forEach((element)=>{%>

                        <% TotalPrice=TotalPrice+(element.Quantity* (element.product.Price-(element.product.Price*element.product.Discount/100)))%>
                        <% totaldiscount=totaldiscount+ element.Quantity*(element.product.Price*element.product.Discount/100)%> 
                        <% totalQ=totalQ+element.Quantity%>
                         <% Price=Price+(element.Quantity* element.product.Price) %> 
                        <li>
                            <a href="single-product.html" class="image"><img src="/AdminAssets/images/addProduct/<%=element.product.Images[0]%>" alt="Cart product Image"></a>
                            <div class="content">
                                <a href="single-product.html" class="title"><%= element.product.ProductName%></a>
                                <span class="quantity-price"><%= element.Quantity%> x <span class="amount">₹ <%=Math.round(element.product.Price-(element.product.Price*element.product.Discount)/100)%></span></span>
                                <a href="#" class="remove">×</a>
                            </div>

                        </li>
                  <% }) %> 
                    <% } %> 
                </ul>
            </div>
            <div class="foot">
                <div class="sub-total">
                    <table class="table">
                        <tbody>
                            <tr>
                                <td class="text-left">Sub-Total :</td>
                                <td class="text-right">₹  <%= Price %> </td>
                            </tr>
                            <tr>
                                <td class="text-left">total Quantity</td>
                                <td class="text-right"><%= totalQ %></td>
                            </tr>
                            <tr>
                                <td class="text-left">Discount</td>
                                <td class="text-right">₹  <%= Math.round(totaldiscount)%></td>
                            </tr>
                            <tr>
                                <td class="text-left">Total :</td>
                                <td class="text-right theme-color">₹  <%= Math.round(TotalPrice)%></td>
                            </tr>
                          
                        </tbody>
                    </table>
                </div>
                <div class="buttons">
                    <a href="/AddCart"class="btn btn-dark btn-hover-primary mb-6">view cart</a>
                   
                </div>
                <p class="minicart-message">Free Shipping on All Orders!</p>
            </div>
        </div>
    </div>
        <!-- OffCanvas Cart End -->
